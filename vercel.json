{
  "version": 2,
  "builds": [
    {
      "src": "Backend/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "Frontend",
      "use": "@vercel/static-build",
      "config": {
        "outputDirectory": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/scraped",
      "dest": "/Backend/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/Frontend/build/$1"
    }
  ]
}